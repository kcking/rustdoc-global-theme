{
    "manifest_version": 2,
    "name": "Rustdoc Global Theme",
    "version": "1.0",
    "description": "Sets your rustdoc theme globally.",
    "permissions": [
        "*://doc.rustlang.org/*",
        "*://docs.rs/*",
        "file:///*",
        "*://localhost/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_title": "Choose Theme",
        "default_popup": "popup.html"
    }
}